{
    "title": "分享文本消息",
    "grouped": true,
    "controllerName": "QQAPIDemoCommonController",
    "sections":
    [
        {
            "elements":
            [
                {
                    "type": "QTextElement",
                    "text": "分享文本消息"
                }
            ]
        },
        {
            "elements":
            [
                {
                    "type": "QEntryElement",
                    "keyboardType": "Default",
                    "title": "title",
                    "placeholder": "填入分享的标题",
                    "textValue": "定向分享标题",
                    "bind": "textValue:binding_title"
                },
                {
                    "type": "QEntryElement",
                    "keyboardType": "Default",
                    "title": "description",
                    "placeholder": "填入分享的描述",
                    "textValue": "定向分享描述",
                    "bind": "textValue:binding_description"
                },
                {
                    "type": "QEntryElement",
                    "keyboardType": "Default",
                    "title": "text",
                    "placeholder": "填入分享的文本内容",
