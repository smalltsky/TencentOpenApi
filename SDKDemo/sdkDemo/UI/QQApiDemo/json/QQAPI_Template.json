                },
                {
                    "type": "QEntryElement",
                    "keyboardType": "Default",
                    "title": "stream url",
                    "placeholder": "填入分享音视频的流媒体URL链接",
                    "textValue": "",
                    "bind": "textValue:binding_streamUrl"
                }
            ]
        },
        {
            "elements":
            [
                {
                    "type": "QButtonElement",
                    "key": "shareBtn",
                    "title": "分享",
                    "controllerAction": "onShareText:"
                }
            ]
        }
    ]
}
